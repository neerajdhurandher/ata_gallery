<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.neeraj_atg_gallery">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_menu_gallery"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_menu_gallery"
        android:supportsRtl="true"
        android:theme="@style/Theme.Neeraj_atg_gallery">
        <activity android:name=".Photo_Details_Activity"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Neeraj_atg_gallery.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>